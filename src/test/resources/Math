math(Integer a , Integer b) returns Integer : add(a, b) { return add.get(0); } | plus2(a) { return plus2.get(0); };
add(Integer a , Integer b) returns Integer : ADD { return a + b; } ;
plus2(Integer a) returns Integer: PLUS2 { return a + 2; } ;

ADD : 'add';
PLUS2 : 'plus2';

