@members {

}

math(Integer a , Integer b) -> Integer : add({Math.abs(a)}, {Math.abs(b)}) { return add.get(0); } | plus2({Math.abs(a)}) { return plus2.get(0); };
add(Integer a , Integer b) -> Integer : ADD { return a + b; } ;
plus2(Integer a) -> Integer: PLUS2 { return a + 2; } ;

ADD : 'add';
PLUS2 : 'plus2';

